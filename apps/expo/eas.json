{
  "build": {
    "development": {
      "distribution": "internal",
      "android": {
        "buildType": "apk"
      },
      "ios": {
        "simulator": true,
        "image": "latest"
      },
      "env": {
        "TAMAGUI_TARGET": "native"
      }
    },
    "production": {
      "distribution": "store",
      "android": {
        "buildType": "app-bundle",
        "node": "18.17.0"
      },
      "ios": {
        "node": "18.17.0"
      },
      "env": {
        "TAMAGUI_TARGET": "native"
      }
    }
  }
}
